{
  "name": "tauri-plugin-ankidroid-js",
  "version": "0.1.0",
  "description": "TypeScript bindings for the Tauri AnkiDroid plugin",
  "author": "Tauri AnkiDroid Plugin Contributors",
  "license": "MIT OR Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "format": "prettier --log-level=error --write .",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "format-lint": "npm-run-all -s format lint",
    "quickfix": "npm-run-all -p format-lint typecheck knip",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "knip": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
