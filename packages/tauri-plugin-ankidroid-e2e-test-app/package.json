{
  "name": "tauri-plugin-ankidroid-e2e-test-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "android:dev": "tauri android dev",
    "android:build": "tauri android build",
    "android:deploy": "export NDK_HOME=/opt/android-sdk/ndk/25.2.9519653 && npm run build && npm run tauri android build -- --debug && adb install -r src-tauri/gen/android/app/build/outputs/apk/universal/debug/app-universal-debug.apk && adb shell am force-stop com.tauri.ankidroid.demo && adb shell am start -n com.tauri.ankidroid.demo/.MainActivity",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "wdio run ./wdio.android.conf.js",
    "test:e2e:setup": "appium server --address 127.0.0.1 --port 4723 --log-level info"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tauri-plugin-ankidroid-js": "*"
